{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "react",
        "@typescript-eslint",
        "eslint-plugin-react",
        "eslint-plugin-react-hooks"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "no-undef": "off",
        "no-unused-vars":"off",
        "array-callback-return": "off"
    }
}
